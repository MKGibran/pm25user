{
  "workbench.colorCustomizations": {
    "minimap.background": "#00000000",
    "scrollbar.shadow": "#00000000",
    "activityBar.background": "#531244",
    "titleBar.activeBackground": "#741A5F",
    "titleBar.activeForeground": "#FDF8FC"
